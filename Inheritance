class Parentclass:
  def __init__(self,name):
    self.name = name

    def name1(self):
      print(f"Hello, my name is {self.name}")

class Childclass(Parentclass):
  def __init__(self,name, age):
    super().__init__(name)
    self.age = age

  def age1(self):
    print(f"I am {self.age} years old")

child=Childclass("Alice",12)
child.name1()
child.age1()
